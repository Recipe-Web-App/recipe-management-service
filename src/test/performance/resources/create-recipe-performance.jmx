          <boolProp name="HTTPSampler.use_keepalive">true</boolProp>
          <boolProp name="HTTPSampler.DO_MULTIPART_POST">false</boolProp>
          <stringProp name="HTTPSampler.embedded_url_re"></stringProp>
          <stringProp name="HTTPSampler.connect_timeout">15000</stringProp>
          <stringProp name="HTTPSampler.response_timeout">45000</stringProp>
          <stringProp name="HTTPSampler.postBodyRaw">true</stringProp>
          <elementProp name="HTTPsampler.postBodyRaw" elementType="HTTPFileArg">
            <stringProp name="HTTPFileArg.postBodyRaw">{
  "title": "Stress Test Recipe ${__threadNum}-${__counter(TRUE,)}-${__time(yyyy-MM-dd-HH-mm-ss)}",
  "description": "Recipe created during stress testing with ${__threadNum} threads",
  "servings": ${__Random(1,12)},
  "preparationTime": ${__Random(1,60)},
  "cookingTime": ${__Random(1,180)},
  "difficulty": "${__chooseRandom(EASY,MEDIUM,HARD)}",
  "ingredients": [
